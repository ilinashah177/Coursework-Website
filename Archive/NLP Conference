      "cell_type": "code",
      "source": [
        "\n",
        "import spacy\n",
        "import json\n",
        "import pylab\n",
        "from IPython.core.display import display, HTML\n",
        "import nltk\n",
        "from tika import parser\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from spacytextblob.spacytextblob import SpacyTextBlob\n",
        "import re\n",
        "from spacy import displacy\n",
        "from collections import Counter\n",
        "\n",
        "\n",
        "nlp = spacy.load(\"en_core_web_sm\")\n",
        "nlp.add_pipe('spacytextblob')\n",
        "\n",
        "speaker_sentiments = []\n",
        "\n",
        "# Iterate through each speaker\n",
        "for speaker in speakers[:21]:  # Process the first 22 speakers\n",
        "\n",
        "    # Find the text spoken by the current speaker (replace with your actual logic to split the transcript)\n",
        "    # This is a placeholder, you need to implement the actual logic for splitting the text by speaker\n",
        "    speaker_text = re.findall(rf\"{speaker}.*?(?=\\d+\\.\\s*[A-Z]|\\Z)\", call['Text'].iloc[0], re.DOTALL)\n",
        "\n",
        "    if speaker_text:\n",
        "      speaker_text = speaker_text[0]\n",
        "      doc = nlp(speaker_text)\n",
        "\n",
        "      # Extract subjectivity and polarity\n",
        "      subjectivity = doc._.blob.subjectivity\n",
        "      polarity = doc._.blob.polarity\n",
        "\n",
        "      speaker_sentiments.append({\n",
        "          'speaker': speaker,\n",
        "          'subjectivity': subjectivity,\n",
        "          'polarity': polarity\n",
        "      })\n",
        "\n",
        "# Create a DataFrame from the speaker sentiments\n",
        "speaker_sentiment_df = pd.DataFrame(speaker_sentiments)\n",
        "speaker_sentiment_df"
      ],
