        "import re\n",
        "\n",
        "speakers = ['Mr. Khalid Taimur Akram',\n",
        " 'His Excellency Ambassador Jiang Zaidong',\n",
        " 'Professor Li Huailiang',\n",
        " 'Senator Sarmad Ali',\n",
        " 'Mr. Awais Manzur Sumra',\n",
        " 'Mr. Muhammad Asim Khan',\n",
        " 'Dr. Hina Shahid',\n",
        " 'Mr. Zafar ul Hassan',\n",
