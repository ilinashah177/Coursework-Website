      "cell_type": "code",
      "source": [
        "\n",
        "import spacy\n",
        "import re\n",
        "from collections import Counter\n",
        "\n",
        "# Text preprocessing\n",
        "text = raw_text.lower()  # Convert to lowercase\n",
        "text = re.sub(r'[^\\w\\s]', '', text)  # Remove punctuation\n",
        "words = text.split()\n",
        "\n",
        "# Remove filler words\n",
        "filler_words = [\"the\", \"and\", \"a\", \"an\", \"in\", \"to\", \"of\", \"is\", \"it\", \"that\", \"for\", \"with\", \"on\", \"as\", \"by\", \"be\", \"this\", \"have\", \"are\", \"we\", \"our\", \"but\", \"has\", \"so\", \"these\", \"but\", \"so\", \"about\", \"which\", \"i\", \"also\", \"its\", \"at\", \"through\", \"can\", \"very\", \"from\", \"or\",\"not\", \"what\", \"they\", \"if\", \"us\", \"think\", \"well\", \"where\", \"when\", \"was\", \"like\", \"under\", \"into\", \"because\", \"some\", \"then\", \"between\", \"only\", \"there\", \"you\", \"been\", \"should\", \"one\"]\n",
        "words = [word for word in words if word not in filler_words]\n",
        "\n",
        "# Count word frequencies\n",
        "word_counts = Counter(words)\n",
        "\n",
        "# Get the top 200 most frequent words\n",
        "top_150_words = word_counts.most_common(150)\n",
        "\n",
        "# Print the results\n",
        "print(\"Top 150 most frequent terms:\")\n",
        "for word, count in top_150_words:\n",
        "    print(f\"{word}: {count}\")"
      ],
