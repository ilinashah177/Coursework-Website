      "cell_type": "code",
      "source": [
        "date = raw['metadata'].get('dcterms:created', 'N/A')  # Use get() with a default value to avoid KeyError\n",
        "title = raw['metadata'].get('dc:title', 'N/A')  # Use get() with a default value to avoid KeyError, or try other keys\n",
        "raw_text = raw['content']\n",
        "\n",
        "print('Date: ', date)\n",
        "print('Title: ', title)\n",
        "print('Word Count: ', len(raw_text))\n",
        "print('Text:')\n",
        "raw_text"
      ],
